<a class="home" href="/">тна Home</a>

<ul>
{% for grp in statuses %}
    <li>
        <span class="grp-name">{{grp.type}}</span>

        <ul>
        {% for item in grp.items %}
            <li>
                <a href="/statuses/{{grp.type}}/{{item.name}}/">
                    {{item.name}}
                </a>
            </li>
        {% endfor %}
        </ul>
    </li>
{% endfor %}
</ul>

